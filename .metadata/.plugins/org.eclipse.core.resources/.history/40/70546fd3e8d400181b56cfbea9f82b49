/**
 * Specifier.java
 * variable and specifier 
 * 
 * 
 */
package print;

/**
 * @author pritimoy
 *
 */
public class Specifier {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		//Declared all variable 
		boolean b = true;
		char c = 'p';
		//byte by =5;
		short s = 1234;
		int i =1234545;
		float f = 10.2f;
		double d = 15.2531485;
		
		//print all variable  value 
		System.out.printf("boolean b =  %b\n", b);
		System.out.printf("Character c =  %c\n", c);
		//System.out.printf("Byte byt =  %d\n", b);
		System.out.printf("Short b =  %d\n", s);
		System.out.printf("Integer i =  %d\n", i);
		System.out.printf("float f =  %d\n", f);
		System.out.printf("double d =  %d\n", d);

	}

}
