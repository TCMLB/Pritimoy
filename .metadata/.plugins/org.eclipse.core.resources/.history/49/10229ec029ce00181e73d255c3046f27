package print;

public class Specialcharacter {//class Specialcharacter is begian
	//Main class is start begin executing

	public static void main(String[] args) {
		System.out.println("\"Special\"");//Print "" special character
		System.out.println("/n");
		System.out.println("()");
		

	}

}
